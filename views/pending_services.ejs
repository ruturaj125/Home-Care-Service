<div id="page-wrapper" style="margin-top: 100px">
<div class="row">
<% for(var i=0; i < data.length; i++) { %>
		 <div class="col-lg-4">
	                    <div class="panel panel-green">
	                        <div class="panel-heading">
	                        </div>
	                        <div class="panel-body">
								<table width:"250">
									<tr>
										<td>Service:</td>
										<td><%=data[i].service%></td>
									</tr>
									<tr>
										<td>Service Date:</td>
										<td><%=data[i].serviceDate%></td>
									</tr>
									<tr>
										<td>Email:</td>
										<td><%=data[i].servant%></td>
									</tr>
									<tr>
										<td>Mobile No:</td>
										<td><%=data[i].mobno%></td>
									</tr>

									<tr>
										<td>Area:</td>
										<td><%=data[i].area%></td>
									</tr>
									<tr>
										<td>Order Date:</td>
										<td><%=data[i].orderedDate%></td>
									</tr>

								</table>
								<a href="/customer/remove_complete?id=<%=i%>" >
		                            <div class="panel-footer">
		                                <span class="pull-left">Done</span>
		                                <span class="pull-right"><i class="fa fa-check"></i></span>
		                                <div class="clearfix"></div>
		                            </div>
		                        </a>

	                    </div>
	                    <!-- /.col-lg-4 -->
	                </div>
					</div>
	<% } %>
	<% if(i==0){ %>
		<h2 align="center">There is no Pending Service of yours</h2>
	<%}%>
</div>
</div>
